{
    "name": "bartlett/graph-plantuml-generator",
    "description": "A PlantUML generator for graph-uml",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Laville",
            "homepage": "https://github.com/llaville",
            "role": "Lead"
        }
    ],
    "require": {
        "php": "^8.1",
        "bartlett/graph-uml": "^1.5",
        "jawira/plantuml-encoding": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "jawira/plantuml": "^1.2024"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "graphp/graphviz": "source",
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "cweagans/composer-patches": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        },
        "bamarni-bin": {
            "bin-links": true,
            "target-directory": "vendor-bin",
            "forward-command": true
        }
    },
    "autoload": {
        "psr-4": {
            "Bartlett\\GraphPlantUml\\": "src/"
        }
    },
    "scripts": {
        "bin": "echo 'bin not installed'"
    }
}
