{
    "name": "bartlett/graph-plantuml-generator",
    "description": "A PlantUML generator for graph-uml",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurent Laville",
            "homepage": "https://github.com/llaville",
            "role": "Lead"
        }
    ],
    "require": {
        "php": "^8.0",
        "bartlett/graph-uml": "^1.3",
        "jawira/plantuml-encoding": "^1.0"
    },
    "require-dev": {
        "jawira/plantuml": "^1.28"
    },
    "minimum-stability": "dev",
    "config": {
        "preferred-install": {
            "graphp/graphviz": "source",
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "autoload": {
        "psr-4": {
            "Bartlett\\GraphPlantUml\\": "src/"
        }
    }
}
